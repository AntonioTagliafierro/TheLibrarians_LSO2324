<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.TheLibrarians_LSO2324" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="colorPrimary">@color/prototype_color1</item>
        <item name="colorPrimaryVariant">@color/prototype_color2</item>


        <item name="android:statusBarColor">@color/prototype_color1</item>
    </style>

    <style name="Theme.TheLibrarians_LSO2324" parent="Base.Theme.TheLibrarians_LSO2324" />
    <style name="Theme.TheLibrarians_LSO2324.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation"> @anim/slide_in_bottom </item>
        <item name="android:windowExitAnimation"> @anim/slide_out_bottom </item>
    </style>
    <style name="Theme.TheLibrarians_LSO2324.AppBarOverlay"
        parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="MySwitch">
        <item name="android:colorControlActivated"> @color/prototype_color1</item>
    </style>
    <style name="Theme.TheLibrarians_LSO2324.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>